  "license": "MIT",
  "private": true,
  "dependencies": {
    "bourbon": "^5.0.0",
    "classnames": "^2.2.5",
    "gsap": "^1.20.3",
    "js-breakpoints": "^1.0.2",
    "lodash": "^4.17.5",
    "font-awesome": "^4.7.0",
    "photoswipe": "^4.1.2",
    "react": "^16.1.0"
  },
  "overrides": {
    "gsap": {
