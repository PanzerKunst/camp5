        "dist/photoswipe-ui-default.js",
        "dist/default-skin/default-skin.png",
        "dist/default-skin/default-skin.svg",
        "dist/default-skin/preloader.gif",
        "src/css/main.scss",
        "src/css/default-skin/default-skin.scss"
